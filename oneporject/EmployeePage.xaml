<Page x:Class="oneporject.EmployeePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:oneporject"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EmployeePage">

    <StackPanel>
        <DataGrid CanUserAddRows="False" x:Name="employeDgr" SelectionChanged="employeDgr_SelectionChanged"/>
        <TextBox x:Name="NameTbx"/>
        <ComboBox DisplayMemberPath="Name_Director" SelectedValuePath="ID_Director" x:Name="DirectorCbx"/>
    </StackPanel>
</Page>
